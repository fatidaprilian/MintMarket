{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/vendor/(.*)",
            "dest": "/public/vendor/$1"
        },
        {
            "src": "/(.*\\.php)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}